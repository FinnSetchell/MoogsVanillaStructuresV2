{
    "schemaVersion": 1,
    "id": "mvs",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "80+ structures made with vanilla blocks and entities bringing life to your world! Discover loot, enemies, & villagers along the way!",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/moogs-voyager-structures",
        "sources": "https://github.com/FinnSetchell/MoogsVoyagerStructures"
    },
    "license": "MIT",
    "icon": "assets/mvs/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.finndog.mvs.MVSFabric"
        ]
    },
    "mixins": [
        "mvs.mixins.json",
        "mvs.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "minecraft": "1.20",
        "java": ">=17"
    },
    "suggests": {
        "another-mod": "*"
    }
}
  